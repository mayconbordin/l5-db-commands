    "keywords": ["laravel", "laravel 5", "database", "commmands"],
    "homepage": "https://github.com/mayconbordin/l5-db-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Maycon Viana Bordin",
            "email": "mayconbordin@gmail.com"
        }
    ],
    "require": {
